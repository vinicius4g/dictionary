{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "jsx": "react",
    "types": [
      "@types/styled-components-react-native"
    ],
    "baseUrl": "./",
    "paths": {
      "@src/*": [
        "./src/*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@screens/*": [
        "./src/screens/*"
      ],
      "@assets/*": [
        "./src/assets/*"
      ],
      "@hooks/*": [
        "./src/hooks/*"
      ],
      "@utils/*": [
        "./src/utils/*"
      ],
      "@styles/*": [
        "./src/styles/*"
      ],
      "@constants/*": [
        "./src/constants/*"
      ],
      "@routes/*": [
        "./src/routes/*"
      ],
      "@models/*": [
        "./src/models/*"
      ],
      "@storage/*": [
        "./src/storage/*"
      ],
      "@services/*": [
        "./src/services/*"
      ],
      "@contexts/*": [
        "./src/contexts/*"
      ],
    },
  },
}